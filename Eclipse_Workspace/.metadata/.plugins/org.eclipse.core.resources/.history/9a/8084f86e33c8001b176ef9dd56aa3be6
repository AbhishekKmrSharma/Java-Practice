import java.io.*;
import java.io.IOException;
public class SimpleFile 
{
	public static void main(String[] args) throws IOException {
		String str="Hello World\n";
		String str1="Welcome";
		FileInputStream a=new FileInputStream("Hello.txt");
		int b;
		
		while((b=a.read())!=-1)
		{
		System.out.print((char)b);
		}
		a.close();

	}
}
