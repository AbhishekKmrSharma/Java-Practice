import java.util.Scanner;
public class MaximumArrayElement 
{
	public static void main(String[] args) 
	{
		Scanner A=new Scanner(System.in);
		int size=A.nextInt();
		
		int[] a=new int[size];
		int temp=0;
		for(int i=0;i<size;i++)
		{
			for(int j=i+1;j<size;j++)
			{
				if(a[i]>a[j])
				{
					temp=a[i];
				}
			}
		}
		System.out.println(temp);
		
		
		
	}
}
