package Array;
import java.util.*;

public class Testtt
{
	public static void main(String[] args)  throws Throwable 
	{
		Scanner s=new Scanner(System.in);
		
		int n=s.nextInt();
		int temp=n;
		int sum=0;
		while(n>0)
		{
			int r=n%10;
			sum=(sum*10)+r;
			n=n/10;
		}
		if(temp==sum)
		{
			System.out.println("Palindrome");
		}
		else
		{
			System.out.println("Not");
		}
	}
}
