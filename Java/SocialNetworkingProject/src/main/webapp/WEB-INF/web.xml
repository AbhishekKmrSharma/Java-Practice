<?xml version="1.0" encoding="UTF-8"?>

<web-app>
<servlet>
<servlet-name>lgn</servlet-name>
<servlet-class>com.social.Login</servlet-class>
</servlet>
<servlet-mapping>
   <servlet-name>lgn</servlet-name>
   <url-pattern>/login</url-pattern>
</servlet-mapping>

<servlet>
<servlet-name>register</servlet-name>
<servlet-class>com.social.Register</servlet-class>
</servlet>
<servlet-mapping>
   <servlet-name>register</servlet-name>
   <url-pattern>/Register</url-pattern>
</servlet-mapping>

<servlet>
 <servlet-name>logout</servlet-name>
 <servlet-class>com.social.LogOut</servlet-class>
</servlet>
<servlet-mapping>
 <servlet-name>logout</servlet-name>
 <url-pattern>/logout</url-pattern>
</servlet-mapping>

<servlet>
 <servlet-name>update</servlet-name>
 <servlet-class>com.social.EditMyProfile</servlet-class>
</servlet>
<servlet-mapping>
 <servlet-name>update</servlet-name>
 <url-pattern>/update</url-pattern>
</servlet-mapping>

 
</web-app>